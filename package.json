{
  "name": "mediawiki",
  "displayName": "MediaWiki",
  "description": "MediaWiki offers syntax highlighting and some features to help editing Wikipedia and MediaWiki projects",
  "version": "0.0.2",
  "publisher": "jasew1",
  "engines": {
    "vscode": "^1.40.0"
  },
  "license": "Apache-2.0",
  "licenses": [
    {
      "type": "Apache-2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0"
    }
  ],
  "keywords": [
    "MediaWiki",
    "Wikipedia"
  ],
  "categories": [
    "Language Packs",
    "Programming Languages",
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jasonwilliams/mediawiki-support"
  },
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "mediawiki",
        "aliases": [
          "Mediawiki",
          "mediawiki"
        ],
        "extensions": [
          ".mediawiki",
          ".wikipedia",
          ".wiki"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "commands": [
      {
        "command": "extension.addWebCitation",
        "title": "Add Web Citation"
      }
    ],
    "grammars": [
      {
        "language": "mediawiki",
        "scopeName": "text.html.mediawiki",
        "path": "./syntaxes/mediawiki.tmLanguage.json",
        "embeddedLanguages": {
          "meta.embedded.block.javascript": "javascript"
        }
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "webpack --mode=development --watch",
    "pretest": "yarn run compile",
    "test": "node ./out/test/runTest.js"
  },
  "icon": "images/MediaWiki_logo_reworked_2.svg.png",
  "devDependencies": {
    "@types/cheerio": "^0.22.15",
    "@types/glob": "^7.1.1",
    "@types/luxon": "^1.21.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.11.7",
    "@types/node-fetch": "^2.5.4",
    "@types/vscode": "^1.40.0",
    "glob": "^7.1.5",
    "mocha": "^6.2.2",
    "ts-loader": "^6.0.4",
    "typescript": "^3.6.4",
    "vscode-test": "^1.2.2",
    "webpack": "^4.38.0",
    "webpack-cli": "^3.3.6"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.3",
    "luxon": "^1.21.3",
    "node-fetch": "^2.6.1"
  }
}
